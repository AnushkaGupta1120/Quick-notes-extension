{
  "manifest_version": 3,
  "name": "Quick Notes for Every Website",
  "version": "1.1",
  "description": "Create multiple sticky notes on any page, synced across devices, with themes and summariser.",
  "permissions": ["storage", "activeTab", "scripting"],
  "icons": {
  "16": "icon16.png",
  "32": "icon32.png",
  "48": "icon48.png",
  "128": "icon128.png"
},
"action": {
  "default_popup": "popup.html",
  "default_icon": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
},
"permissions": [
  "storage",
  "activeTab",
  "scripting"
]


  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_end"
  }
]

}
